{
  "expo": {
    "name": "Gushi Stories",
    "slug": "gushi-stories",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/thumbnail.png",
    "scheme": "gushi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danielwilderdev.gushi",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "appleTeamId": "X2HJ883WPZ",
      "associatedDomains": [
        "applinks:gushi.spacenoodle.ai"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#020617"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.danielwilderdev.gushi",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#082F49"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8bad05d6-d793-4557-95c5-bc4a4ff074f7"
      }
    },
    "owner": "danielwilderdev"
  }
}
